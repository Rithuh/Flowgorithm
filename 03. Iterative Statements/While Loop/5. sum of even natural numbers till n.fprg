    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, i, sum" type="Integer" array="False" size=""/>
            <do expression="n&lt;1">
                <output expression="&quot;Enter a natural number : &quot;" newline="True"/>
                <input variable="n"/>
            </do>
            <assign variable="sum" expression="0"/>
            <assign variable="i" expression="2"/>
            <while expression="i&lt;=n">
                <assign variable="sum" expression="sum+i"/>
                <assign variable="i" expression="i+2"/>
            </while>
            <output expression="&quot;The sum of  even natural numbers till &quot;&amp;n&amp;&quot; = &quot;&amp;sum" newline="True"/>
        </body>
    </function>
</flowgorithm>
